package com.part1.composingobjects;

public class ComposingObject {
	public static void main(String[] args) {
		Father f = new Son();
		f.runn(1);
	}
}

class Father{
	private void runn() {
		System.out.println(123);
	}
}

class Son extends Father{
	public void runn() {
		System.out.println("456");
	}
}
