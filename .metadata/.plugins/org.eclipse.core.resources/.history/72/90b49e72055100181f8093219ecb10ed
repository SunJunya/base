package com.heima.design;

public class Design_Singleton {
	public static void main(String[] args) {
		new Thread(){
			public void run(){
				int i=0;
				while(i<=1000){
					System.out.println(getName()+"...11111111111111111111");
					i++;
				}
			}
		}.start();
		
		new Thread(){
			public void run(){
				int i=0;
				while(i<=1000){
					System.out.println(getName()+"...bb");
					i++;
				}
			}
		}.start();
	}
}

